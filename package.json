{
  "name": "@rareelements/iceaxe-core",
  "version": "0.1.0",
  "description": "Unofficial AWS Glacier Client library",
  "license": "MIT",
  "scripts": {
    "prepack": "tsc --project ./tsconfig.json",
    "test": "tsc --noEmit && jest",
    "clean": "del-cli lib dist coverage && jest --clearCache",
    "build": "npm run clean && npm run build:cleanup && tsc --project ./tsconfig.json",
    "build:cleanup": "del-cli \"lib/**/?(*.)test.@(ts|js)\"",
    "lint": "tslint --project ."
  },
  "keywords": [
    "AWS Glacier"
  ],
  "main": "lib/index.js",
  "files": [
    "dist/",
    "lib/"
  ],
  "author": {
    "name": "rareelement",
    "email": "support@rareelement.ca"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rareelement/iceaxe-core"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.5",
    "@types/yargs": "^15.0.5",
    "aws-sdk": "^2.673.0",
    "aws-sdk-mock": "^5.1.0",
    "del-cli": "^3.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "ts-jest": "^26.0.0",
    "tslint": "^5.12.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "read-chunk": "^3.2.0",
    "winston": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "src/**/*.ts": "npm run lint"
  }
}
